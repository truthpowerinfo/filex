@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Truth Power Info.</title>

    <meta http-equiv="x-ua-compatible" content="ie=edge" />

    <meta name="description" content="" />
    <link rel="shortcut icon" type="image/x-icon" href="~/assets/images/favicon.svg" />

    <!-- ========================= CSS here ========================= -->
    <link rel="stylesheet" href="~/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/assets/css/LineIcons.3.0.css" />
    <link rel="stylesheet" href="~/assets/css/animate.css" />
    <link rel="stylesheet" href="~/assets/css/tiny-slider.css" />
    <link rel="stylesheet" href="~/assets/css/glightbox.min.css" />
    <link rel="stylesheet" href="~/assets/css/main.css" />
</head>
<body data-new-gr-c-s-check-loaded="14.1047.0" data-gr-ext-installed="">
    <!--[if lte IE 9]>
          <p class="browserupgrade">
            You are using an <strong>outdated</strong> browser. Please
            <a href="https://browsehappy.com/">upgrade your browser</a> to improve
            your experience and security.
          </p>
        <![endif]-->

    <div class="preloader" style="opacity: 0; display: none;">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>


    <header class="header navbar-area others-pages">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    <div class="nav-inner">
                        <!-- Start Navbar -->
                        <nav class="navbar navbar-expand-lg">
                            <a class="navbar-brand" href="~/Home/index">
                                <img src="~/assets/images/logo/logo.svg" alt="Logo">
                            </a>
                            <button class="navbar-toggler mobile-menu-btn" type="button" data-bs-toggle="collapse"
                                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                                    aria-expanded="false" aria-label="Toggle navigation">
                                <span class="toggler-icon"> </span>
                                <span class="toggler-icon"> </span>
                                <span class="toggler-icon"> </span>
                            </button>
                            <div class="collapse navbar-collapse sub-menu-bar " style="" id="navbarSupportedContent">
                                <ul id="nav" class="navbar-nav ms-auto">
                                    <li class="nav-item">
                                        <a href="~/Home/Privacy" aria-label="Toggle navigation"> Privacy Policy</a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="~/Home/Terms" aria-label="Toggle navigation"> Terms</a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="~/Home/Disclaimer" aria-label="Toggle navigation"> Disclaimer</a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="~/Home/Support" aria-label="Toggle navigation"> Support</a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="~/home/tools" aria-label="Toggle navigation"> Tools</a>
                                    </li>
                                </ul>
                            </div> <!-- navbar collapse -->
                        </nav>
                        <!-- End Navbar -->
                    </div>
                </div>
            </div> <!-- row -->
        </div> <!-- container -->
    </header>
    <div class="breadcrumbs">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6 col-md-6 col-12">
                    <div class="breadcrumbs-content">
                        <h1 class="page-title">@ViewBag.Title</h1>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-12">
                    <ul class="breadcrumb-nav">
                        <li><a href="~/@ViewContext.RouteData.Values["controller"].ToString()">@ViewContext.RouteData.Values["controller"].ToString()</a></li>
                        <li>@ViewBag.Title</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    @RenderBody()

    <!-- Start Footer Area -->
    <footer class="footer section">
        <!-- Start Footer Top -->
        <div class="footer-top">
            <div class="container">
                <div class="inner-content">
                    <div class="row">
                        <div class="col-lg-4 col-md-6 col-12">
                            <!-- Single Widget -->
                            <div class="single-footer f-about">
                                <div class="logo">
                                    <a href="index.html">
                                        <img src="~/assets/images/logo/logo.svg" alt="#">
                                    </a>
                                </div>
                                <p> 100% Free & Secure online documents converter tool  </p>
                                <p class="copyright-text">
                                    <span> &copy; @DateTime.Now.Year - Filex.</span> Made with ❤️ by<a href="https://truthpowerinfo.com/" rel="nofollow" target="_blank"> Truth Power Info.</a>
                                </p>
                            </div>
                            <!-- End Single Widget -->
                        </div>
                        <div class="col-lg-2 col-md-6 col-12">
                            <!-- Single Widget -->
                            <div class="single-footer f-link">
                                <h3> Support</h3>
                                <ul>
                                    <li> <a href="~/Home/Privacy"> Privacy & Plocies </a> </li>
                                    <li> <a href="~/Home/Disclaimer"> Disclaimer</a> </li>
                                    <li> <a href="~/Home/Terms"> Terms of Service</a> </li>
                                    <li> <a href="~/Home/Support"> Support</a> </li>
                                </ul>
                            </div>
                            <!-- End Single Widget -->
                        </div>
                        <div class="col-lg-4 col-md-6 col-12">
                            <!-- Single Widget -->
                            <div class="single-footer newsletter">
                                <h3> Subscribe</h3>
                                <p> Subscribe to our newsletter for the latest updates</p>
                                <div class="newsletter-form">
                                    <input name="EMAIL" placeholder="Email address" required="required" type="email">
                                    <div class="button">
                                        <button class="sub-btn">
                                            <i class="lni lni-envelope"> </i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <!-- End Single Widget -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- / End Footer Top -->
    </footer>
    <!-- / End Footer Area -->
    <!-- ========================= scroll-top ========================= -->
    <a href="#" class="scroll-top">
        <i class="lni lni-chevron-up"> </i>
    </a>
    <!-- ========================= JS here ========================= -->
    <script src="~/assets/js/bootstrap.min.js"> </script>
    <script src="~/assets/js/wow.min.js"> </script>
    <script src="~/assets/js/tiny-slider.js"> </script>
    <script src="~/assets/js/glightbox.min.js"> </script>
    <script src="~/assets/js/count-up.min.js"> </script>
    <script src="~/assets/js/main.js"> </script>
   
    <div id="volume-booster-visusalizer">
        <div class="sound">
            <div class="sound-icon"></div>
            <div class="sound-wave sound-wave_one"></div>
            <div class="sound-wave sound-wave_two"></div>
            <div class="sound-wave sound-wave_three"></div>
        </div>
        <div class="segments-box">
            <div data-range="1-20" class="segment"><span></span></div>
            <div data-range="21-40" class="segment"><span></span></div>
            <div data-range="41-60" class="segment"><span></span></div>
            <div data-range="61-80" class="segment"><span></span></div>
            <div data-range="81-100" class="segment"><span></span></div>
        </div>
    </div>
</body>
</html>